{
  "name": "infrastructure",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "build": "yarn audit && tsc",
    "clean": "yarn run clean-artifacts && yarn install && yarn run scan",
    "clean-artifacts": "rm -rf ./node_modules && rm -rf ./yarn.lock",
    "deploy": "cdk deploy --require-approval never",
    "destroy": "cdk destroy",
    "diff": "cdk diff",
    "lint": "npx eslint",
    "synth": "cdk synth",
    "scan": "yarn outdated && yarn audit"
  },
  "devDependencies": {
    "@eslint/js": "^9.12.0",
    "@types/node": "^22.7.5",
    "eslint": "^9.12.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.6.3",
    "typescript-eslint": "^8.8.1"
  },
  "dependencies": {
    "@typescript-eslint/eslint-plugin": "^8.8.1",
    "@typescript-eslint/parser": "^8.8.1",
    "aws-cdk-lib": "^2.162.1",
    "constructs": "^10.4.1"
  }
}